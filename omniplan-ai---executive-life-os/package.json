{
  "name": "omniplan-ai---executive-life-os",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron-main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:electron:dev": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "start:electron": "electron .",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "react": "^19.2.3",
    "lucide-react": "^0.562.0",
    "@google/genai": "^1.38.0",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "electron": "^26.0.0",
    "electron-builder": "^24.6.0",
    "cross-env": "^7.0.3"
  },
  "build": {
    "appId": "com.omniplan.app",
    "files": [
      "dist/**",
      "electron-main.cjs",
      "package.json"
    ],
    "win": {
      "target": ["portable"],
      "certificateFile": null,
      "certificatePassword": null,
      "signingHashAlgorithms": null,
      "sign": null
    }
  }
}
